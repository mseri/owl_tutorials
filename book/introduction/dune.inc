(alias
 (name   runtest)
 (deps   (:x README.md)
         (package mdx)
         (package core)
         (package owl-top)
         (package ppx_jane)
         (:y1 ../../examples/code/introduction/hello.ml)
         (:y0 ../../examples/code/introduction/world.ml)
         prelude.ml)
 (action (progn
           (run ocaml-mdx test --prelude=prelude.ml --direction=to-md %{x})
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected)
           (diff? %{x} %{x}.corrected))))
