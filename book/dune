(alias
  (name html)
  (deps algodiff.html
        architecture.html
        backend.html
        cgraph.html
        constant.html
        dataframe.html
        diffequation.html
        introduction.html
        linear-algebra.html
        maths.html
        matrix.html
        ndarray.html
        neural-network.html
        neural-ode.html
        nlp.html
        optimisation.html
        prologue.html
        slicing.html
        stats.html
        visualisation.html
        zoo.html))

(rule
  (targets algodiff.html)
  (deps    algodiff/README.md)
  (action  (run mdx output %{deps} -o %{targets})))

